<form version="1.1">

  <label>Home Page</label>
  <description>Please type all the respective values</description>
  <fieldset submitButton="false">
        <input type="dropdown" token="lookup" searchWhenChanged="true">
      <label>Environment</label>
      <fieldForLabel>stack</fieldForLabel>
      <fieldForValue>stack</fieldForValue>
      <search>
        <query>|rest splunk_server=local services/server/info |fields serverName | rex field=serverName ".*?\.(?&lt;stack&gt;.*?)\." |fields stack</query>
        <earliest>-15m</earliest>
        <latest>now</latest>
      </search>
    </input>
    <input type="text" token="idx" searchWhenChanged="true">
      <label>Indexer</label>
    </input>
    <input type="text" token="type" searchWhenChanged="true">
      <label>Authentication(SAML/LDAP)</label>
    </input>
  </fieldset>
